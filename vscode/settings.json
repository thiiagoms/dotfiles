{
    "editor.cursorBlinking": "expand",
    "editor.cursorWidth": 2,
    "editor.fontFamily": "MesloLGM Nerd Font",
    "editor.fontSize": 16,
    "editor.renderWhitespace": "all", 
    "editor.lineHeight": 30,
    "editor.letterSpacing": 0.5,
    "editor.rulers": [
        120
